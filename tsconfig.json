{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*",
        "./*"
      ],
      "@/components/*": [
        "./src/components/*",
        "./components/*"
      ],
      "@/lib/*": [
        "./src/lib/*",
        "./lib/*"
      ],
      "@/server/*": [
        "./src/server/*",
        "./server/*"
      ],
      "@/generated/*": [
        "./src/generated/*",
        "./generated/*"
      ]
    },
    "baseUrl": "."
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next\\dev/types/**/*.ts",
    ".next\\dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "supabase/functions/**",
    "supabase/sql/**",
    "analysis/**"
  ]
}
